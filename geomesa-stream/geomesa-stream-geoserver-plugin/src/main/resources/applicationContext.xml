<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="streamDataStorePanel" class="org.geoserver.web.data.resource.DataStorePanelInfo">
        <property name="id" value="streamDataStorePanel"/>
        <property name="factoryClass" value="org.locationtech.geomesa.stream.datastore.StreamDataStoreFactory"/>
        <property name="iconBase" value="org.geoserver.web.GeoServerApplication"/>
        <property name="icon" value="img/icons/geosilk/database_vector.png"/>
        <property name="componentClass" value="org.locationtech.geomesa.stream.plugin.StreamDataStoreEditPanel"/>
    </bean>

</beans>
